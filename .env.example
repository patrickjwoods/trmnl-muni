# Your 511.org API key (get one at https://511.org/open-data/token)
API_KEY_511=your_api_key_here

# Stop IDs separated by semicolons
# Find stop IDs at https://511.org or from GTFS data
# Example: Church & Duboce inbound and outbound
MUNI_STOPS=15726;15727

# Max departures per line (default: 3)
MAX_DEPARTURES=3
